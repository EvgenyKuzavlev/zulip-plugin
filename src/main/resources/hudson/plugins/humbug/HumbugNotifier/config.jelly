<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <!--
    This jelly script is used for per-project configuration.
    See global.jelly for a general discussion about jelly script.
  -->
    <f:entry title="Custom Stream Name" description="Optional. Use to send notifications to a stream other than the default (${descriptor.getStream()})" help="${rootURL}/plugin/humbug/help-projectConfig-stream.html">
      <f:textbox name="humbugStream" value="${instance.getConfiguredStreamName()}"/>
    </f:entry>
</j:jelly>
